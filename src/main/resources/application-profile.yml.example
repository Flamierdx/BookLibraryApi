spring:
  datasource:
    url: your_jdbc_url
    username: your_db_username
    password: your_db_password
    driver-class-name: your_db_driver_class_name
app:
  jwt:
    access:
      secret: your_secret_key
      expiration: your_expiration_in_ms
    refresh:
      secret: your_secret_key
      expiration: your_expiration_in_ms